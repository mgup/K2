											<xs:annotation>
												<xs:documentation>ИД источника финансирования (Справочник 15 "Источник финансирования")</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="EducationLevelID" type="xs:unsignedInt"  minOccurs="0">
