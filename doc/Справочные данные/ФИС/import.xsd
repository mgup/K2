															<xs:all>
																<xs:element name="Application">
																	<xs:annotation>
																		<xs:documentation>Заявление, включаемое в список</xs:documentation>
																	</xs:annotation>
																	<xs:complexType>
																		<xs:all>
																			<xs:element name="ApplicationNumber">
