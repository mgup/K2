																	</xs:annotation>
																	<xs:complexType>
																		<xs:choice>
																			<xs:element name="FinSourceEduForm" maxOccurs="unbounded">
																				<xs:annotation>
																					<xs:documentation>Условия приема по текущей реализации</xs:documentation>
																				</xs:annotation>
																				<xs:complexType>
																					<xs:all>
																						<xs:element name="EducationalLevelID" type="xs:unsignedInt">
																							<xs:annotation>
																								<xs:documentation>ИД уровня образования (справочник 2)</xs:documentation>
																							</xs:annotation>
																						</xs:element>
																						<xs:element name="EducationFormID" type="xs:unsignedInt">
																							<xs:annotation>
																								<xs:documentation>ИД формы обучения (справочник №14)</xs:documentation>
																							</xs:annotation>
																						</xs:element>
																						<xs:element name="CompetitiveGroupID" type="xs:string">
																							<xs:annotation>
																								<xs:documentation>UID конкурсной группы</xs:documentation>
																							</xs:annotation>
																						</xs:element>
																						<xs:element name="DirectionID" type="xs:unsignedInt">
																							<xs:annotation>
																								<xs:documentation>ИД направления подготовки (справочник №10)</xs:documentation>
																							</xs:annotation>
																						</xs:element>
																					</xs:all>
																				</xs:complexType>
																			</xs:element>
																			<xs:element name="FinSourceAndEduForm" maxOccurs="unbounded">
																				<xs:annotation>
																					<xs:documentation>Условия приема по спецификации</xs:documentation>
																				</xs:annotation>
																				<xs:complexType>
																					<xs:all>
																						<xs:element name="EducationalLevelID" type="xs:unsignedInt">
																							<xs:annotation>
																								<xs:documentation>ИД уровня образования (справочник 2)</xs:documentation>
																							</xs:annotation>
																						</xs:element>
																						<xs:element name="EducationalFormID" type="xs:unsignedInt">
																							<xs:annotation>
																								<xs:documentation>ИД формы обучения (справочник №14)</xs:documentation>
																							</xs:annotation>
																						</xs:element>
																						<xs:element name="CompetitiveGroupID" type="xs:string">
																							<xs:annotation>
																								<xs:documentation>UID конкурсной группы</xs:documentation>
																							</xs:annotation>
																						</xs:element>
																						<xs:element name="DirectionID" type="xs:unsignedInt">
																							<xs:annotation>
																								<xs:documentation>ИД направления подготовки (справочник №10)</xs:documentation>
																							</xs:annotation>
																						</xs:element>
																					</xs:all>
																				</xs:complexType>
																			</xs:element>
																		</xs:choice>
																	</xs:complexType>
																</xs:element>
															</xs:all>
														</xs:complexType>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
									</xs:all>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:all>
		</xs:complexType>
	</xs:element>
	<xs:element name="Root">
		<xs:complexType>
			<xs:all>
